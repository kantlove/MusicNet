<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="50.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="musicnet.controller.TopBarController">
    <children>
        <Button fx:id="buttonMedia" layoutX="14.0" layoutY="13.0" mnemonicParsing="false"
                prefHeight="25.0" prefWidth="60" onAction="#onButtonMediaClicked" text="Play"/>
        <Button fx:id="buttonDown" layoutX="350.0" layoutY="13.0" mnemonicParsing="false" onAction="#showDownloadDialog"
                prefHeight="25.0" prefWidth="100.0" text="Download (0)"/>
        <Button fx:id="buttonUp" layoutX="475.0" layoutY="13.0" mnemonicParsing="false" onAction="#showUploadDialog"
                prefHeight="25.0" prefWidth="100.0" text="Upload (0)"/>
        <Label fx:id="labelName" layoutX="80.0" layoutY="17.0" />
    </children>
</AnchorPane>
